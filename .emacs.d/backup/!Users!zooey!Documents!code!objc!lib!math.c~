int pow(int x, int n) {
  int result = 1;
  for (int i = 0; i < n; i++){
    result *= x;
  }
  return result;
}

int parseInt(char* string) {
  int i = 0;
  int order = 0;
  int result = 0;
  // Look at our string, find out how "long" it is
  while (1) {
    if (*(string + (i+1)) == NULL) break;
    order++;
    i++;
  }
  i = 0;
  while (1) {
    char c = *(string + i);
    if (c == NULL) break;
    int num = c - 48; // 0 has ascii value 48
    result += (num * pow(10, order));
    order--;
    i++;
  }
  return result;
}
